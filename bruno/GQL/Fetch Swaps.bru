meta {
  name: Fetch Swaps
  type: graphql
  seq: 3
}

post {
  url: {{url}}/opus/api/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  {
    totalReturnSwaps {
      edges {
        node {
          uuid
          id
          name
          updatedAt
          manuallyUpdatedAt
          asset {
            id
            name
            uuid
          }
        }
      }
    }
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
